[
   {
      "id": "bar-0",
      "name": "bar-niri",
      "layer": "top",
      "mode": "dock",
      "height": 26,
      "margin": 0,
      "spacing": 1,
      "output": "DP-1",
      "include": [
         "$HOME/.config/waybar/modules.jsonc"
      ],
      "modules-left": [
         "hyprland/workspaces",
         "custom/space",
         "custom/spotify"
      ],
      "modules-center": [
         // "niri/window",
         "clock#calendar"
         // "clock#amsterdam"
      ],
      "modules-right": [
         "memory",
         "cpu",
         "disk",
         "temperature",
         "custom/separator",
         "tray",
         "custom/separator",
         "privacy",
         "custom/updates",
         "idle_inhibitor",
         "custom/notifications",
         "custom/separator",
         "bluetooth",
         "pulseaudio",
         "battery"
      ]
   }
]
